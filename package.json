{
  "name": "create-freenit",
  "version": "0.0.4",
  "private": false,
  "dependencies": {
    "axios": "^0.21.1",
    "freenit": "^0.0.27",
    "prop-types": "^15.7.2",
    "radium": "^0.26.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "http-proxy-middleware": "^1.0.5",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^12.0.0",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/{mock,service,styles}.js",
      "!src/{pages,templates}/*/{index,initial}.js",
      "!src/{App,index,theme}.js",
      "!src/serviceWorker.js",
      "!src/setupProxy.js"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testResultsProcessor=jest-junit --color --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bin": "create-freenit"
}
